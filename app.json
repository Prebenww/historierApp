    "ios": {
      "buildNumber": "1.0.3",
      "bundleIdentifier": "no.studentproblemer1.historier",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "package": "no.studentproblemer1.historier",
      "permissions": ["NOTIFICATIONS"]
    }
  }
}
